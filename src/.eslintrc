{
    "extends": ["eslint:recommended"],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "experimentalObjectRestSpread": true
        }
    },
    "parser": "babel-eslint",
    "env": {
        "browser": true
    },
    "plugins": ["react"],
    "rules": {
        "react/jsx-uses-vars": "error"
    },
    "globals": {
        "Promise": true
    }
  }
  